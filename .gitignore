/node_modules
/.pnp
.pnp.js
.yarn/install-state.gz

# testing
/coverage
/playwright-report
/playwright/.cache
/test-results
*.lcov

# production
/build
/dist

# misc
.DS_Store
*.pem

# debug
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*
.pnpm-debug.log*

# local env files
.env*.local
.env.local
.env.development.local
.env.test.local
.env.production.local

# vercel
.vercel

# typescript
*.tsbuildinfo
next-env.d.ts

# vite
.vite_cache/
vite.config.js.timestamp-*
vite.config.ts.timestamp-*

# TypeScript compilation artifacts
# Ignore ALL TypeScript declaration files - they are auto-generated
*.d.ts
**/*.d.ts

# Ignore compiled JavaScript files and source maps in src
src/**/*.js
src/**/*.js.map

# Keep only necessary config files
!src/**/*.config.js
!src/**/*.setup.js
!vite.config.js
!vitest.config.js
!playwright.config.js

