{
  "extends": ["react-app"],
  "rules": {
    "@typescript-eslint/no-explicit-any": 0,
    "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "@typescript-eslint/no-this-alias": 0,
    "no-extra-boolean-cast": 0,
    "no-sparse-arrays": 0,
    "no-fallthrough": 0,
    "no-redeclare": 0
  },
  "overrides": [
    {
      "files": ["**/*.ts", "**/*.tsx", "**/*.d.ts"],
      "rules": {
        "@typescript-eslint/no-explicit-any": 0
      }
    }
  ]
}